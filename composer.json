	"type": "library",
	"license": "GPL-3.0-only",
	"keywords": ["continuous integration", "continuous delivery", "deploy", "tool", "deploid"],
	"authors": [
		{
			"name": "userator",
			"email": "userator@ya.ru",
			"homepage": "https://github.com/userator"
		}
	],
	"require": {
		"php": ">=5.6",
		"symfony/console": "v3.4.17",
		"symfony/process": "v3.4.17",
		"monolog/monolog": "1.23.0"
	},
	"require-dev": {
		"phpunit/phpunit": "5.7.*"
